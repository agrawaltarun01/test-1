<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Moviez</servlet-name>
		<servlet-class>com.movieztalk.server.HomeCarouselServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Moviez</servlet-name>
		<url-pattern>/carousel</url-pattern>
	</servlet-mapping>
	
	<servlet>
        <servlet-name>MoviezPlot</servlet-name>
        <servlet-class>com.movieztalk.server.MoviePlotServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MoviezPlot</servlet-name>
        <url-pattern>/movieplot</url-pattern>
    </servlet-mapping>
	
	<servlet>
		<servlet-name>MovieGame</servlet-name>
		<servlet-class>com.movieztalk.server.MovieGameServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MovieGame</servlet-name>
		<url-pattern>/moviegame</url-pattern>
	</servlet-mapping>
	
	<servlet>
    <servlet-name>MovieGameInitialize</servlet-name>
    <servlet-class>com.movieztalk.server.MovieGameInitializerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MovieGameInitialize</servlet-name>
    <url-pattern>/moviegameinit</url-pattern>
  </servlet-mapping>

    <servlet>
		<servlet-name>HelloServlet</servlet-name>
		<servlet-class>com.movieztalk.server.TestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloServlet</servlet-name>
		<url-pattern>/HelloServlet</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
